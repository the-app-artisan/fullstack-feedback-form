// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model Feedback {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  category  String
  rating    Int
  message   String
  notify    Boolean
  createdAt DateTime @default(now())
}
